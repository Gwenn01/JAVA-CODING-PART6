import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter Rows: ");
		int rows = s.nextInt();
		
		for(int i = 0; i < rows; i++){
			for(int j = 0; j <= i; j++){
				System.out.print("*");
			}
			System.out.println();
		}
		for(int i = 0; i < rows; i++){
			for(int j = 0; j < rows-i-1; j++){
				System.out.print("*");
			}
			System.out.println();
		}
		
	}
}